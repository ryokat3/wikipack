{
  "name": "wikipack",
  "version": "0.28.0",
  "description": "A HTML to pack all markdown files and assets in a single HTML with viewer",
  "main": "index.js",
  "scripts": {
    "build": "webpack-cli && shx cp -r pages/* dist",
    "build-prod": "webpack-cli --mode='production'",
    "test": "webpack-cli --config='webpack.config.test.ts'",
    "dev-server": "webpack-cli serve --config='webpack.config.dev-server.ts'",
    "clean": "shx rm -rf dist/* src/tmp tmp"
  },
