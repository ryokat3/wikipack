    "@types/mocha": "^10.0.7",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/webpack": "^5.28.5",
    "@types/wicg-file-system-access": "^2023.10.5",
    "chai": "^5.1.1",
    "glob": "^11.0.0",
    "html-webpack-plugin": "^5.6.0",
    "inline-source": "^8.0.3",
    "mocha": "^10.6.0",
    "shx": "^0.3.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
